{
    "name": "aws-sdk-kotlin",
    "needs_compiler": false,
    "packages": [],
    "variables": {
        "gradlew": "{source_dir}/gradlew -p {source_dir}"
    },
    "build_steps": [
        "{gradlew} assemble"
    ],
    "test_steps": [
        "{gradlew} publishToMavenLocal",
        "{gradlew} test allTests",
        "{gradlew} testAllProtocols"
    ],
    "imports": [
        "JDK8"
    ],
    "build_dir": "target/build",
    "env": {
        "JAVA_HOME": "{java_home}"
    },
    "hosts": {
        "ubuntu": {
            "packages": [
                "openjdk-8-jdk-headless"
            ]
        },
        "debian": {
            "packages": [
                "openjdk-8-jdk-headless"
            ]
        }
    },
    "upstream": [
        {
            "name": "smithy-kotlin"
        },
        {
            "name": "aws-crt-kotlin"
        }
    ],
    "variants": {
        "build-all-services": {
            "!build_steps": [
                "{gradlew} --no-daemon :codegen:sdk:bootstrap -Paws.services=-ec2,-location,-elasticbeanstalk,-marketplacecommerceanalytics",
                "{gradlew} assemble --parallel"
            ],
            "!test_steps": [
                "{gradlew} test allTests"
            ]
        },
        "no-tests": {
            "!test_steps": []
        }
    }
}
